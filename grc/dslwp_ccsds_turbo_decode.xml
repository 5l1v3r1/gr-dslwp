<block>
  <name>CCSDS Turbo Decode</name>
  <key>dslwp_ccsds_turbo_decode</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.ccsds_turbo_decode($base, $octets, $code_type, $iterations, $sigma)</make>
  <param>
    <name>Base</name>
    <key>base</key>
    <value>223</value>
    <type>int</type>
  </param>
  <param>
    <name>Octets</name>
    <key>octets</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Code Type</name>
    <key>code_type</key>
    <value>2</value>
	<type>enum</type>
	<option>
		<name>Rate 1/2</name>
		<key>2</key>
	</option>
	<option>
		<name>Rate 1/3</name>
		<key>3</key>
	</option>
	<option>
		<name>Rate 1/4</name>
		<key>4</key>
	</option>
	<option>
		<name>Rate 1/6</name>
		<key>6</key>
	</option>
  </param>
  <param>
    <name>Iterations</name>
    <key>iterations</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>Sigma</name>
    <key>sigma</key>
    <value>0.707</value>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
