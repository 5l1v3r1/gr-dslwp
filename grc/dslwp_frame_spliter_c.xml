<block>
  <name>Frame Spliter C</name>
  <key>dslwp_frame_spliter_c</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.frame_spliter_c($key, $frame_length)</make>
  <param>
    <name>Key</name>
    <key>key</key>
    <value>payload_start</value>
    <type>string</type>
  </param>
  <param>
    <name>Frame Length</name>
    <key>frame_length</key>
    <value>3576</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
