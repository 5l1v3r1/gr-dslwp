<block>
  <name>Set Gain by Tag CC</name>
  <key>dslwp_set_gain_by_tag_cc</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.set_gain_by_tag_cc($k, $gain)</make>
  <param>
    <name>K</name>
    <key>k</key>
    <value>512</value>
    <type>float</type>
  </param>
  <param>
    <name>Gain</name>
    <key>gain</key>
    <value>1</value>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
