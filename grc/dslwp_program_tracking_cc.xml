<block>
  <name>Program Tracking CC</name>
  <key>dslwp_program_tracking_cc</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.program_tracking_cc($enable, $path, $lon, $lat, $alt, $fc, $samp_rate, $txrx, $verbose)</make>
  <param>
    <name>Enable</name>
    <key>enable</key>
    <value>True</value>
    <type>bool</type>
    <option>
		<name>On</name>
		<key>True</key>
    </option>
    <option>
		<name>Off</name>
		<key>False</key>
    </option>
  </param>
  <param>
    <name>Path</name>
    <key>path</key>
		<type>file_open</type>
  </param>
  <param>
    <name>Longtitude</name>
    <key>lon</key>
    <type>float</type>
  </param>
  <param>
    <name>Latitude</name>
    <key>lat</key>
    <type>float</type>
  </param>
  <param>
    <name>Altitude (m)</name>
    <key>alt</key>
    <type>float</type>
  </param>
  <param>
    <name>Center Frequency (Hz)</name>
    <key>fc</key>
    <type>float</type>
  </param>
  <param>
    <name>Sample Rate (Hz)</name>
    <key>samp_rate</key>
    <type>raw</type>
  </param>
  <param>
    <name>TXRX</name>
    <key>txrx</key>
    <value>False</value>
    <type>bool</type>
    <option>
		<name>TX</name>
		<key>True</key>
    </option>
    <option>
		<name>RX</name>
		<key>False</key>
    </option>
  </param>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <value>False</value>
    <type>bool</type>
    <option>
		<name>On</name>
		<key>True</key>
    </option>
    <option>
		<name>Off</name>
		<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
