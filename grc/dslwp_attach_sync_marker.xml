<block>
  <name>Attach Sync Marker</name>
  <key>dslwp_attach_sync_marker</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.attach_sync_marker($marker, $data_format)</make>
  <param>
    <name>Marker</name>
    <key>marker</key>
    <type>raw</type>
  </param>
  <param>
    <name>Data Format</name>
    <key>data_format</key>
    <value>1</value>
	<type>enum</type>
	<option>
		<name>Bits</name>
		<key>1</key>
	</option>
	<option>
		<name>Packed bits</name>
		<key>2</key>
	</option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
