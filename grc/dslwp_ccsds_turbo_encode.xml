<block>
  <name>CCSDS Turbo Encode</name>
  <key>dslwp_ccsds_turbo_encode</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.ccsds_turbo_encode($base, $octets, $code_type)</make>
  <param>
    <name>Base</name>
    <key>base</key>
    <value>223</value>
    <type>int</type>
  </param>
  <param>
    <name>Octets</name>
    <key>octets</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Code Type</name>
    <key>code_type</key>
    <value>2</value>
	<type>enum</type>
	<option>
		<name>Rate 1/2</name>
		<key>2</key>
	</option>
	<option>
		<name>Rate 1/3</name>
		<key>3</key>
	</option>
	<option>
		<name>Rate 1/4</name>
		<key>4</key>
	</option>
	<option>
		<name>Rate 1/6</name>
		<key>6</key>
	</option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
